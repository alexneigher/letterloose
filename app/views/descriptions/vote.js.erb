<%- if @source == 'index' %>
  // this is the descriptions index page
  $(".<%= dom_id(@description) %>").html("<%= j(render partial: 'shared/description', locals: {description: @description}) %>")

<% else %>
  // this is the tweak show page
  $("#<%= dom_id(@tweak) %>").html("<%= j(render partial: 'tweaks/show', locals: {tweak: @tweak}) %>")
<% end %>

var thumbsUp = new Audio("/sounds/upvote.mp3");
thumbsUp.play();
